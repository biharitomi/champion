	</servlet-mapping>
	<servlet-mapping>
	  <servlet-name>Faces Servlet</servlet-name>
	  <url-pattern>*.xhtml</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
	  <welcome-file>faces/index.xhtml</welcome-file>
